{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#89b4fa",
          "style": "plain",
          "template": "<b></b> {{ .Name }} ",
          "type": "shell"
        },
        {
          "foreground": "#fab387",
          "properties": {
            "folder_icon": "",
            "home_icon": "",
            "style": "agnoster_full"
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground_templates": [
            "{{ if gt .Code 0 }}#f38ba8{{ end }}"
          ],
          "properties": {
            "always_enabled": false
          },
          "style": "plain",
          "template": "{{ if gt .Code 0 }}<b>{{ .Code }}</b>{{ end }}",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#b4befe",
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Version }}{{ if .Version }}<b>{{ .Version }}</b> {{ end }}{{ end }}{{ if .Name }}{{ .Name }} {{ end }}{{ end }}",
          "type": "project"
        },
        {
          "foreground": "#dd0031",
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "angular"
        },
        {
          "foreground": "#f79919",
          "style": "plain",
          "template": " {{.Profile}}{{if .Region}}@{{.Region}}{{end}}",
          "type": "aws"
        },
        {
          "foreground": "#0072c6",
          "style": "plain",
          "template": "ﴃ {{ .EnvironmentName }}",
          "type": "az"
        },
        {
          "foreground": "#ffbf00",
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "azfunc"
        },
        {
          "foreground": "#0092d1",
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "cds"
        },
        {
          "foreground": "#2e5797",
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "cf"
        },
        {
          "foreground": "#2e5797",
          "style": "plain",
          "template": " {{ .Org }}/{{ .Space }} ",
          "type": "cftarget"
        },
        {
          "foreground": "#cc1010",
          "style": "plain",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "cmake"
        },
        {
          "foreground": "#010101",
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "crystal"
        },
        {
          "foreground": "#00d2b8",
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "dart"
        },
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "ﯤ {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "deno"
        },
        {
          "foreground": "#1072b8",
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "dotnet"
        },
        {
          "foreground": "#674078",
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "elixir"
        },
        {
          "foreground": "#16b9fd",
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "flutter"
        },
        {
          "foreground": "#ffce45",
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{.Project}} :: {{.Account}}{{ end }}",
          "type": "gcp"
        },
        {
          "foreground": "#69d7e4",
          "style": "plain",
          "template": "ﳑ {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "go"
        },
        {
          "foreground": "#5e5086",
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "haskell"
        },
        {
          "foreground": "#f34336",
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "java"
        },
        {
          "foreground": "#9259a3",
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "julia"
        },
        {
          "foreground": "#ff8a00",
          "style": "plain",
          "template": "洞{{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "kotlin"
        },
        {
          "foreground": "#466bb4",
          "properties": {
            "parse_kubeconfig": true
          },
          "style": "plain",
          "template": "ﴱ {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}",
          "type": "kubectl"
        },
        {
          "foreground": "#02027d",
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "lua"
        },
        {
          "foreground": "#5fa04e",
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": "<#cb3837></> ",
            "yarn_icon": "<#2c8ebb></> "
          },
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }}{{ end }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "node"
        },
        {
          "foreground": "#cb3837",
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "npm"
        },
        {
          "foreground": "#002f56",
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "nx"
        },
        {
          "foreground": "#3a3c5b",
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "perl"
        },
        {
          "foreground": "#777bb3",
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "php"
        },
        {
          "foreground": "#3773a5",
          "properties": {
            "display_mode": "context"
          },
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "python"
        },
        {
          "foreground": "#2369bd",
          "style": "plain",
          "template": "R {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "r"
        },
        {
          "foreground": "#ab1401",
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "ruby"
        },
        {
          "foreground": "#f74c00",
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "rust"
        },
        {
          "foreground": "#f36537",
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "swift"
        },
        {
          "foreground": "#5f42e8",
          "style": "plain",
          "template": "{{.WorkspaceName}}{{ if .Version }} {{ .Version }}{{ end }} ",
          "type": "terraform"
        },
        {
          "foreground": "#6b5c8e",
          "style": "plain",
          "template": "V {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "vala"
        },
        {
          "foreground": "#42b983",
          "style": "plain",
          "template": " make {{ if .Error }}{{ .Error }}{{ else }}{{ if .Full }}<b>{{ .Full }}</b> {{ end }}{{ end }}",
          "type": "xmake"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#a6e3a1",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#f9e2af{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f5c2e7{{ end }}",
            "{{ if gt .Ahead 0 }}#585b70{{ end }}",
            "{{ if gt .Behind 0 }}#585b70{{ end }}"
          ],
          "properties": {
            "branch_icon": " ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#f38ba8>  {{ .Staging.String }}</>{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "newline": false,
      "segments": [
        {
          "foreground": "#f5c2e7",
          "style": "plain",
          "type": "text",
          "template": " Astolfo uwu"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#cba6f7",
          "properties": {
            "linux": "",
            "macos": "",
            "windows": ""
          },
          "style": "plain",
          "template": "{{.Icon}} ",
          "type": "os"
        },
        {
          "foreground": "#fab387",
          "style": "plain",
          "template": " ",
          "type": "root"
        },
        {
          "foreground": "#a6e3a1",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#f38ba8{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "ﬀ ",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#a6e3a1",
    "foreground_templates": [
      "{{ if gt .Code 0 }}#f38ba8{{ end }}"
    ],
    "template": "ﬀ "
  },
  "version": 2
}

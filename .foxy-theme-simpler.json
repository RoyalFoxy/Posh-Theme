{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#cdd6f4",
          "style": "plain",
          "template": "\uf120 {{ .Name }} ",
          "type": "shell"
        },
        {
          "foreground": "#cdd6f4",
          "properties": {
            "folder_icon": " \uf07b ",
            "home_icon": "\uf7dd",
            "style": "agnoster_full"
          },
          "style": "plain",
          "template": "\uf07b {{ .Path }} ",
          "type": "path"
        },
        {
          "type": "exit",
          "style": "plain",
          "foreground": "#cdd6f4",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#f38ba8{{ end }}"
          ],
          "template": "{{ if gt .Code 0 }}{{ .Code }}{{ end }}",
          "properties": {
            "always_enabled": true
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "project",
          "style": "plain",
          "foreground": "#b4befe",
          "template": "{{ if .Error }}{{ .Error }}{{ else }}{{ if .Version }}\uf487 {{.Version}} {{ end }}{{ if .Name }}{{ .Name }}{{ end }}{{ end }} "
        },
        {
          "foreground": "#f74c00",
          "style": "plain",
          "template": "\ue7a8 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "rust"
        },
        {
          "foreground": "#5fa04e",
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": "<#cb3837>\ue5fa</> ",
            "yarn_icon": "<#2c8ebb>\uf61a</> "
          },
          "style": "plain",
          "template": "\ue718 {{ if .Error }}{{ .Error }}{{ else }}{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }}{{ end }}{{ .Full }}{{ end }} ",
          "type": "node"
        },
        {
          "type": "php",
          "style": "plain",
          "foreground": "#777bb3",
          "template": "\ue73d {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "foreground": "#f34336",
          "style": "plain",
          "template": "\ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "java"
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "#3773a5",
          "template": "\uE235 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "properties": {
            "display_mode": "context"
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#f9e2af{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f5c2e7{{ end }}",
            "{{ if gt .Ahead 0 }}#585b70{{ end }}",
            "{{ if gt .Behind 0 }}#585b70{{ end }}"
          ],
          "foreground": "#a6e3a1",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#f38ba8> \uf046 {{ .Staging.String }}</>{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#cdd6f4",
          "properties": {
            "linux": "\ue712",
            "macos": "\ue711",
            "windows": "\ue70f"
          },
          "trailing_diamond": " ",
          "style": "diamond",
          "template": "{{.Icon}}",
          "type": "os"
        },
        {
          "foreground": "#fab387",
          "trailing_diamond": " ",
          "style": "diamond",
          "template": "\uf292",
          "type": "root"
        },
        {
          "foreground": "#a6e3a1",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#f38ba8{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\ufb00 ",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#a6e3a1",
    "foreground_templates": [
      "{{ if gt .Code 0 }}#f38ba8{{ end }}"
    ],
    "template": "\ufb00 "
  },
  "version": 2
}